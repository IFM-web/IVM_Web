
@{
    ViewData["Title"] = "Visitor Dashboard";
    Layout = "~/Views/Shared/_Layout1.cshtml";
    @inject Microsoft.AspNetCore.Http.IHttpContextAccessor httpContextAccessor
}
<div class="row px-5">
  
    <div class="top-header text-center">
        <img src="data:image/png;base64,@httpContextAccessor.HttpContext.Session.GetString("Companylogo")" alt="head-logo" class="img-fluid" width="20%">
    </div>
    <h3 class="text-center">Visitor Dashboard</h3>

    <div class="row my-3" style="overflow-y:auto">
        <div class="row my-3">
            <div class="col-5">
                <input type="text" placeholder="Search " id="filterInput" class="form-control" />
            </div>
              <div class="col-2">
                <button class="btn btn-success" onclick="Refresh()">Refresh</button>
            </div>
        </div>
        <div class="col-12" id="PrintDiv"></div>
    </div>
  
</div>
<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script src="~/js/VisitorDashboard.js" asp-append-version="true" ></script>