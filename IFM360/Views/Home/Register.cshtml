@model VMS_Web.Models.login;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <!-- Meta-Information -->
    <title>User Login - iVM360</title>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="~/Images/logo.png" type="image/x-icon">
    <!-- Vendor: Bootstrap Stylesheets http://getbootstrap.com -->
    <!-- Our Website CSS Styles -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/icons.css" rel="stylesheet" />
    <link href="~/css/sass.css" rel="stylesheet" />
    <link href="~/css/main.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" />
    <link rel="icon" type="image/png" href="~/upload/logo/logo.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/7.0.0/css/all.min.css" integrity="sha512-DxV+EoADOkOygM4IR9yXP8Sb2qwgidEmeqAEmDKIOfPRQZOWbXCzLC6vjbZyy0vPisbH2SyW27+ddLVCN+OMzQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

</head>

<body>


    <div class="account-user-sec" style="overflow-y: auto;">
        <div class="account-sec">
            <div class="account-top-bar">
                <div class="container">
                    <div class="logo">

                        <a href="#/" title="">
                            <img src="~/upload/logo/logo.png" style="width: 10%; margin-top: -4%; margin-bottom: -4%;" />&nbsp;&nbsp;
                            <span style="color: blue; font-size: large;">iVM</span>
                            <span style="color: red; font-size: large;">360</span>
                        </a>
                    </div>
                    <ul class="account-header-link">
                        <li><a title="" href="~/Home/Register">Register</a></li>
                        <li><a title="" href="~/Home/Login">Login</a></li>
                        <li><a title="">Schedule a Demo</a></li>
                    </ul>
                </div>
            </div>
            <div class="acount-sec">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="contact-sec">
                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="widget-title">
                                            <h3>User Registration</h3>
                                            <span>Please enter the below details</span>
                                        </div>
                                        <!-- Widget title -->
                                        <div class="account-form">
                                            <form method="post" action="./register.aspx" id="ctl00" class="ng-pristine ng-valid">
                                                <div class="aspNetHidden">
                                                    <input type="hidden" name="__VIEWSTATE" id="__VIEWSTATE" value="X1L/yVrfba/PjvJLCfp684QOtMCqj9uNarlRpC1IQKqXceqmLcnPtKoxi6yX78kQnxXXGFd4OmbzDjFndOnjx0nPKa3JnXIdnjhbbYFBVNqVo48YJ4qC0nSt+6o842twhR/Nyzei2HR0EzpnYVkWzQ==">
                                                </div>

                                                <div class="aspNetHidden">

                                                    <input type="hidden" name="__VIEWSTATEGENERATOR" id="__VIEWSTATEGENERATOR" value="CBA42DF4">
                                                </div>
                                                <div id="reg1" class="row">
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="email" type="text" id="email" class="input-style" placeholder="Work Email ID" maxlength="50" required="">
                                                        <span id="messageEmail"></span>
                                                    </div>
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="mobile" type="text" id="mobile" placeholder="Mobile Number" maxlength="10" minlength="10" onkeypress="return isNumberKey(event)" required="">
                                                        <span id="messageMobile"></span>
                                                    </div>
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="fName" type="text" id="fName" placeholder="First Name" maxlength="20" minlength="2" onkeypress="allowAlphaNumeric(event)" required="">
                                                    </div>
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="lName" type="text" id="lName" placeholder="Last Name" maxlength="20" minlength="2" onkeypress="allowAlphaNumeric(event)" required="">
                                                    </div>
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="password" type="password" id="password" placeholder="Password" maxlength="20" minlength="8" required="">
                                                    </div>
                                                    <div class="col-md-6 feild" style="top:0px;left:0px;">
                                                        <input name="cPassword" type="password" id="cPassword" placeholder="Confirm Password" required="">
                                                        <span id="messagePassword"></span>
                                                    </div>
                                                    <div class="col-md-12" style="top:0px;left:0px;width:100%">
                                                        <label style="font-size: 12px;color: brown; font-weight: 600;">
                                                            Password must contain atleast 1 Capital Alphabet, 1 Number, 1 Special Character and Minimum 8 Character long.
                                                        </label>
                                                    </div>
                                                    <div class="feild col-md-6" style="float: right; margin-right: -30%;top:0px;left:0px;width:50%">
                                                        <input type="submit" name="Regfun1" value="Next" onclick="return regfun1();" id="Regfun1" style="background: darkslateblue;">
                                                    </div>
                                                </div>




                                            </form>
                                        </div>
                                        <div class="more-option">
                                            <span>OR</span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="creat-an-account">
                                            <span>Do you have an account?</span>
                                            <a href="~/Home/Login" title="" style="background: darkslateblue;">Login Now</a>

                                        </div>
                                        <!-- Create an account -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <footer>
                <div class="container">
                    <p>©@DateTime.Now.Year  V19 Softech Pvt. Ltd.  </p>
                </div>
            </footer>
        </div>
        <!-- Account Sec -->
    </div>



    <script src="https://maps.google.com/maps/api/js?sensor=false"></script>

    <script type="text/javascript">

        if(window.location.hostname == "localhost"){
            localStorage.setItem("Url","")
        }else{
             localStorage.setItem("Url","/IvmCore")
        }


        function mylogin() {

            if (email.value.length > 0) {
                var emailValid = ValidateEmail(email.value);
                if (emailValid == true) {
                    return true;
                } else {
                    document.getElementById("email").focus();
                    return false;
                }
            } else {
                document.getElementById("email").focus();
                return false;
            }
        }



    </script>

</body>
</html>