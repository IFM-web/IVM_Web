
@{
    ViewData["Title"] = "Employee BulkUpload";
    Layout = "~/Views/Shared/_LayoutSuperAdmin.cshtml";
}


<div class="row px-2 px-md-5">
    <div class="header my-3">
        <h3 class="text-center">Manage Employee</h3>
    </div>
    <div class="body mb-3">


        <div class="form">
         
            <div class="row mt-4">
                <div class="col-md-4">
                    <label>Excel File</label>
                    <input class="form-control" id="excelFile" type="file" />
                </div>

                <div class="col-md-12 mt-4">
                    <a href="~/Sample/Employee_Sample_Data_Sheet.xlsx" class="btn btn-myColor">Download Sample Employee Data</a>
                    <button class="btn btn-myColor" id="btnexcelsave">Upload Employee Data</button>
                    <a href="~/AdminArea/EmployeeList" class="btn btn-myColor">Close</a>
                </div>

            </div>
       
            <div id="FormatDiv" class="mt-4"></div>
        </div>
    </div>
</div>
<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>

<script src="~/js/AdminArea/EmployeeUpload.js" asp-append-version="true"></script>
<script src="~/js/site.js"></script>


