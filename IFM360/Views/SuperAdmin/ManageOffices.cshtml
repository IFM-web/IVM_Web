
@{
    ViewData["Title"] = "Manage Offices";
    Layout = "~/Views/Shared/_LayoutSuperAdmin.cshtml";
}

<div class="row px-2 px-md-5">
    <div class="header my-3">
        <h3 class="text-center">Manage Offices</h3>
    </div>
    <div class="body mb-3">


        <div class="form">
            <div class="row mt-4">
                <div class="col-md-6 mb-3">
                    <input hidden id="HidId" value="0" />
                    <label>Office Name</label>
                    <input type="text" id="txtofficename" name="office Name" class="form-control mandatory">
                </div>

                <div class="col-md-6 mb-3">                 
                    <label>Office Address</label>
                    <input type="text" id="txtofficeAddress" name="office Address" class="form-control mandatory">
                </div>

                <div class="col-md-6 mb-3">              
                    <label>Office State</label>
                    <input type="text" id="txtofficState" name="office State" class="form-control mandatory">
                </div>

                <div class="col-md-6 mb-3">              
                    <label>Is Shared Space</label>
                    <select id="isSpace" name="Is Shared Space" class="form-control ">
                        <option value="1">Yes</option>
                        <option value="0">No</option>
                    </select>
                  
                </div>
                <div class="col-md-4 mt-4">
                    <button onclick="addOffice()" id="btnname" class="btn btn-myColor">Add New Office</button>
                    <a href="~/SuperAdmin/ManageOfficesList" id="btnname" class="btn btn-myColor">Show All Office</a>
                </div>
            </div>
            @* <p>Below is the list of Department-</p>
            <hr /> *@
            
        </div>
    </div>
    </div>


    <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>

 <script src="~/js/SuperAdmin/Manageffice.js" asp-append-version="true"></script> 
 <script src="~/js/site.js"></script>




